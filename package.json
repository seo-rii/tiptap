  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "license": "MIT",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seorii/tiptap.git"
  }
}
